{
    "title": "Angular 从入门到实践",
    "author": "cipchk",
    "plugins": [
        "image-captions",
        "github-buttons",
        "github-issue-feedback"
    ],
    "pluginsConfig": {
        "image-captions": {
            "attributes": {
                "width": "600"
            },
            "caption": "_PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
        },
        "github-buttons": {
            "repo": "cipchk/angular-practice",
            "types": [
                "star",
                "watch"
            ],
            "size": "small"
        },
        "github-issue-feedback": {
            "repo": "cipchk/angular-practice"
        }
    }
}